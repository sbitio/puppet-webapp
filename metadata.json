{
  "name": "sbitio-webapp",
  "version": "0.2.0",
  "author": "SB IT Media, S.L.",
  "summary": "Wrapper to create resources that compound webapps",
  "license": "MIT",
  "source": "http://github.com/sbitio/puppet-webapp",
  "project_page": "https://github.com/sbitio/puppet-webapp",
